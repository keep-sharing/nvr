/*TableView*/
.TableView
{
    outline: none;
    border: 1px solid #3E3E3E;
    color: #FFFFFF;
    background: #2E2E2E;
}
.TableView::item
{
    border: 0px;
    background: #2E2E2E;
}
.TableView::item:alternate
{
    background: #3D3D3D;
}
.TableView::item:selected
{
    background: rgb(104,134,158);
}
.TableView[style="black"] HeaderView
{
    border:none;
    background: #5F5F5F;
    color: white;
}
.TableView[style="black"] HeaderView::section
{
    border-right: 1px solid #2E2E2E;
    background: #5F5F5F;
    color:white;
}

TableView::indicator
{
    width: 18px;
    height: 18px;
}
TableView::indicator:unchecked
{
    border-image: url(:/common/common/checkbox-unchecked.png);
}
TableView::indicator:unchecked:hover
{
    border-image: url(:/common/common/checkbox-unchecked-hover.png);
}
TableView::indicator:unchecked:disabled
{
    border-image: url(:/common/common/checkbox-unchecked-disable.png);
}
TableView::indicator:checked
{
    border-image: url(:/common/common/checkbox-checked.png);
}
TableView::indicator:checked:disabled
{
    border-image: url(:/common/common/checkbox-checked-disable.png);
}
TableView::indicator:indeterminate
{
    border-image: url(:/common/common/checkbox-partiallychecked.png);
}
TableView::indicator:indeterminate:hover
{
    border-image: url(:/common/common/checkbox-partiallychecked-hover.png);
}

/*Horizontal Slider*/
QSlider::groove:horizontal
{
        border: 1px solid #999999;
        height: 5px;
        background: #A8A8A8;
}
QSlider::handle:horizontal
{
        background: #0AA8E3;
        border: 1px solid #5c5c5c;
        width: 16px;
        margin: -6px -2;
        border-radius: 8px;
}
