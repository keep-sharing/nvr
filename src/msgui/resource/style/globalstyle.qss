/*black combobox*/
.ComboBoxBlack
{
    height: 30px;
    border: 1px solid #5F5F5F;
    color: #FFFFFF;
    background-color: #5F5F5F;
}
.ComboBoxBlack:hover
{
    border: 1px solid #00A2E8;
}
.ComboBoxBlack:drop-down
{
    border: 0px;
}
.ComboBoxBlack::down-arrow
{
    image: url(:/common/common/drop-down-white.png);
}
.ComboBoxBlack QAbstractItemView
{
    outline: none;
    border: 1px solid #FFFFFF;
    background-color: #000000;
}
.ComboBoxBlack QAbstractItemView::item
{
    height: 30px;
    color: #FFFFFF;
}
.ComboBoxBlack QAbstractItemView::item:hover
{
    background-color: #00A2E8;
}
.ComboBoxBlack QAbstractItemView::item:selected
{
    background-color: #00A2E8;
}
QScrollBar:vertical
{
    border: 0px;
    background: #5F5F5F;
    width: 20px;
    margin: 20px 0 20px 0;
}
QScrollBar::handle:vertical
{
    background: #7F7F7F;
    min-height: 20px;
}
QScrollBar::add-line:vertical
{
    border: 0px;
    background: #696A6F;
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:pressed
{
    background: #2E2E2E;
}
QScrollBar::sub-line:vertical
{
    border: 0px;
    background: #696A6F;
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:pressed
{
    background: #2E2E2E;
}
QScrollBar::up-arrow:vertical
{
    image: url(:/common/common/up-arrow.png);
}
QScrollBar::down-arrow:vertical
{
    image: url(:/common/common/down-arrow.png);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    background: none;
}

