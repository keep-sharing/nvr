	emmc: mmc@f01a0000 {
		#address-cells = <2>;
		#size-cells = <2>;
		compatible = "nvt,nvt_mmc2";
		reg = <0x2 0xf01a0000 0x0 0x1000>;
		interrupts = <GIC_SPI 61 IRQ_TYPE_LEVEL_HIGH>;
		max-frequency = <48000000>;
		max-voltage = <18000>;
		bus-width = <8>;
		driving = <2 2 2 3 2 2 5 4 4 6 5 5>;
		cd_gpio = <0 GPIO_FALLING_EDGE GPIO_INTERRUPT>;
		no-sdio;
		no-sd;
		/*ro_gpio = <D_GPIO(3) GPIO_RISING_EDGE GPIO_POLLING>;*/
		/*power_en = <D_GPIO(4) GPIO_RISING_EDGE>;*/

		/* Enable below to turn on hs200; recommended frequency: 96000000, 160000000 */
		/*
		mmc-hs200-1_8v;
		*/

		/* Enable below to turn on hs400; recommended freqency: 160000000 */
		/*
		clk_parent = "pll6";
		mmc-hs400-1_8v;
		mmc-hs400-enhanced-strobe;
		*/
	};
