		reg =  <0x2 0xf0480000 0x0 0x1000
				0x2 0xf04e0000 0x0 0x1000
				0x2 0xf08b0000 0x0 0x8000>;
		interrupts = <GIC_SPI 69 IRQ_TYPE_LEVEL_HIGH>;
