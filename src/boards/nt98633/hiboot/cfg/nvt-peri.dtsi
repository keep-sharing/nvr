		interrupts = <GIC_SPI 29 IRQ_TYPE_LEVEL_HIGH>;
		clock-frequency = <24000000>;
		nvt-devname = "spi_nand.0";
	};

	nor: nor@f0180000 {
		#address-cells = <2>;
		#size-cells = <2>;
		compatible = "nvt,nvt_spinor";
		reg = <0x2 0xf0180000 0x0 0x1000>;
		interrupts = <GIC_SPI 29 IRQ_TYPE_LEVEL_HIGH>;
		clock-frequency = <48000000>;
		nvt-devname = "spi_nor.0";
	};
*/
	usb3: u3hst@2,f0580000 {
	        compatible = "nvt,nvt_usb3xhci";
		reg = <0x2 0xf0580000 0x0 0x10000
				0x2 0xf05f0000 0x0 0x4000>;
		interrupts = <GIC_SPI 28 IRQ_TYPE_LEVEL_HIGH>;
		power_gpio = <P_GPIO(33) 1>;
	};

	gpio: gpio@2,f0040000 {
		compatible = "nvt,nvt_gpio";
		reg = <0x2 0xf0040000 0x0 0x10000>;
		interrupts = <GIC_SPI 24 IRQ_TYPE_LEVEL_HIGH
				GIC_SPI 62 IRQ_TYPE_LEVEL_HIGH
				GIC_SPI 63 IRQ_TYPE_LEVEL_HIGH
				GIC_SPI 80 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-controller;
		interrupt-parent = <&gic>;
		#interrupt-cells = <3>;
		#gpio-cells = <2>;
	};

	eth0@2,f0200000 {
		compatible = "nvt,synopsys_eth";
		reg = <0x2 0xf0200000 0x0 0x10000>;
		interrupts = <GIC_SPI 12 IRQ_TYPE_LEVEL_HIGH>;
		sp-clk = <1>;
		ref-clk-out = <0>;
		mac-address = [00 00 00 00 00 00];
	};

	eth1@2,f0210000 {
		compatible = "nvt,synopsys_eth";
		reg = <0x2 0xf0210000 0x0 0x10000>;
		interrupts = <GIC_SPI 11 IRQ_TYPE_LEVEL_HIGH>;
		sp-clk = <1>;
		ref-clk-out = <0>;
		mac-address = [00 00 00 00 00 00];
		fixed-link {
			speed = <1000>;
			full-duplex;
		};
	};

	nvt_sata100_1: sata@2f0b8 {
		compatible = "novatech,nvt_sata100";
		reg =  <0x2 0xf0b80000 0x0 0x1000
				0x2 0xf0bc0000 0x0 0x1000
				0x2 0xf0be0000 0x0 0x8000>;
		interrupts = <GIC_SPI 13 IRQ_TYPE_LEVEL_HIGH>;
		device-id = <1>;
		comm-rst = <0>;
	};
	

	nvt_sata100_2: sata@2f0b9 {
		compatible = "novatech,nvt_sata100";
		reg =  <0x2 0xf0b90000 0x0 0x1000
				0x2 0xf0bd0000 0x0 0x1000
				0x2 0xf0bf0000 0x0 0x8000>;
		interrupts = <GIC_SPI 14 IRQ_TYPE_LEVEL_HIGH>;
		device-id = <2>;
		comm-rst = <1>;
	};

